bool isSubsetSum(int set[], int n, int sum) 
{ 
	
	bool subset[n + 1][sum + 1]; 


	for (int i = 0; i < n+1; i++) { 
		for (int j = 0; j < sum+1; j++) { 
          if(i==0)
            subset[i][j]=false;
          if(j==0)
            subset[i][j]=true;
          else
          {
          
			if (j < set[i - 1]) 
				subset[i][j] = subset[i - 1][j]; 
			if (j >= set[i - 1]) 
				subset[i][j] = subset[i - 1][j] 
							|| subset[i - 1][j - set[i - 1]]; 
          }
		} 
	} 



	return subset[n][sum]; 
} 
