int countNodesinLoop(struct Node *head)
{
     // Code here
     #include<vector>
     vector<Node*>v;
     v.push_back(head);
    
     while(head->next)
     {
         if(find(v.begin(),v.end(),head->next)==v.end())
         {
             v.push_back(head);
             head=head->next;
         }
         else
         {
             break;
         }
         
     }
     if(head->next==NULL)
     return 0;
     else
     {
         Node *p=head->next;
         head->next=NULL;
         int c=0;
         while(p)
         {
             c++;
             p=p->next;
         }
         return c;
     }
     
}
