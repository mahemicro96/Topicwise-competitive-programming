int countNodesinLoop(struct Node *head)
{
     // Code here
     set<Node*>s;
     while(head)
     {
         if(s.find(head)==s.end())
         s.insert(head);
         else
         break;
         head=head->next;
     }
     if(!head)
     return 0;
     else
     {
         Node *p=head->next;
         head->next=NULL;
         int c=0;
     while(p)
     {
         c++;
         p=p->next;
     }
     return c;
     }
}
