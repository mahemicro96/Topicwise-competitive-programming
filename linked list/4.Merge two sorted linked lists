Node* sortedMerge(Node* head1,   Node* head2)
{
    Node *e=NULL,*p=NULL;
    while(head1 && head2)
    {
        if(head1->data<head2->data)
        {
            Node *temp=new Node(head1->data);
            if(e==NULL)
            {
                e=temp;
                p=temp;
            }
            else
            {
                p->next=head1;
                p=head1;
            }
            head1=head1->next;
        }
         else
        {
            Node *temp=new Node(head2->data);
            if(e==NULL)
            {
                e=temp;
                p=e;
            }
             else
            {
                p->next=head2;
                p=head2;
            }
            
            head2=head2->next;
        }
    }
    if(head1)
    {
        p->next=head1;
    }
     if(head2)
    {
        p->next=head2;
        
    }
    return e;
}
