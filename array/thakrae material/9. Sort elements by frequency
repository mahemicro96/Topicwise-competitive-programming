Print the elements of an array in the decreasing frequency if 2 numbers have same frequency 
then print the one which came first.

#include<iostream>
#include<map>
#include<vector>
#include<algorithm>
using namespace std;
struct ele{
	int x;
	int ind;
	int freq=0;
};
bool comp(struct ele x,struct ele y)
{
	if(x.freq==y.freq)
	return x.ind<y.ind;
	else
	return x.freq>y.freq;
}

main()
{
	int n;
	cin>>n;
	ele y[n];
		vector<int>v;
	for(int i=0;i<n;i++)
	{
		
		cin>>y[i].x; 
		v.push_back(y[i].x);
			
	}
	
	for(int i=0;i<n;i++)
	{
		y[i].freq=count(v.begin(),v.end(),y[i].x);
	y[i].ind=find(v.begin(),v.end(),y[i].x)-v.begin();
	
	}
	sort(y,y+n,comp);
	for(int i=0;i<n;i++)
	cout<<y[i].x<<" "<<y[i].freq<<" "<<y[i].ind<<endl;
	
}



input:
8
2 5 2 8 5 6 8 8
