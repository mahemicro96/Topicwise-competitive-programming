#include<iostream>
#include<vector>
#include<climits>
#include<algorithm>
using namespace std;
int func(vector<int>v)
{
    int sum1,sum2;
    sum1=INT_MIN;
    sum2=0;
    for(int i=0;i<v.size();i++)
    {
        sum2=sum2+v[i];
        if(sum2>sum1)
        sum1=sum2;
        if(sum2<0)
        sum2=0;
    }
    return sum1;
}
main()
{
    int n;cin>>n;
    while(n--)
    {
    
        int m;
        cin>>m;
        vector<int>v(m);
        for(int i=0;i<m;i++)
        cin>>v[i];
        int x=func(v);
        cout<<x<<endl;
    }
}
